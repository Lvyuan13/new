                    <Button Background="Yellow" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor21" Width="24" ToolTip="Yellow"></Button>
                    <Button Background="SandyBrown" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor22" Width="24" ToolTip="SandyBrown"></Button>
                </StackPanel>
                <StackPanel Name="stackPanel2" Orientation="Vertical" HorizontalAlignment="Right" Width="26">
                    <Button Background="Black" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor23" Width="24" ToolTip="Black" />
                    <Button Background="CadetBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor24" Width="24" ToolTip="CadetBlue" />
                    <Button Background="DodgerBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor25" Width="24" ToolTip="DodgerBlue" />
                    <Button Background="MediumSlateBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor26" Width="24" ToolTip="MediumSlateBlue" />

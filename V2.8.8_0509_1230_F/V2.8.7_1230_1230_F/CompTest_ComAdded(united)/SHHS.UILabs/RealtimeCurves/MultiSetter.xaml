        ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid Background="#ebe9ed">
        <TabControl Name="tabControl" Margin="0,0,75,0" Background="#ebe9ed" >
        </TabControl>
        <GroupBox Header="{DynamicResource Color}" HorizontalAlignment="Right" Margin="0,134,5,15" Name="groupBox5" Width="65" >

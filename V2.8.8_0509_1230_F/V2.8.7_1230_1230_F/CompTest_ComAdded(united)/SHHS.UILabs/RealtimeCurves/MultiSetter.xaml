                    <Button Background="Orchid" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor38" Width="24" ToolTip="Orchid" />
                    <Button Background="SpringGreen" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor39" Width="24" ToolTip="SpringGreen" />
                    <Button Background="DarkOrange" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor40" Width="24" ToolTip="DarkOrange" />
                    <Button Background="LightCoral" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor41" Width="24" ToolTip="LightCoral" />
                    <Button Background="IndianRed" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor42" Width="24" ToolTip="IndianRed" />
                    <Button Background="Red" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor43" Width="24" ToolTip="Red" />
                    <Button Background="DarkRed" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor44" Width="24" ToolTip="DarkRed" />
                </StackPanel>
            </Grid>
        </GroupBox>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,32,7,0" Name="btnOK" VerticalAlignment="Top" Width="60" Click="button_Click" Content="{DynamicResource OK}"></Button>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,62,7,0" Name="btnCancel" VerticalAlignment="Top" Width="60" Click="button_Click" Content="{DynamicResource Cancel}"></Button>
        <Button Height="23" HorizontalAlignment="Right" Margin="0,92,7,0" Name="btnApply" VerticalAlignment="Top" Width="60" Click="button_Click" Content="{DynamicResource Apply}"></Button>
    </Grid>
</Window>
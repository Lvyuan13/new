                <StackPanel Name="stackPanel1" Orientation="Vertical" HorizontalAlignment="Left" Width="26">
                    <Button Background="Red" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor1" Width="24" ToolTip="Red"></Button>

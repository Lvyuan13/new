                    <Button Background="Turquoise" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor33" Width="24" ToolTip="Turquoise" />
                    <Button Background="LimeGreen" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor34" Width="24" ToolTip="LimeGreen" />
                    <Button Background="PaleVioletRed" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor35" Width="24" ToolTip="PaleVioletRed" />

                    <Button Background="Plum" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor5" Width="24" ToolTip="Plum"></Button>
                    <Button Background="Violet" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor6" Width="24" ToolTip="Violet"></Button>
                    <Button Background="Magenta" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor7" Width="24" ToolTip="Magenta"></Button>
                    <Button Background="DarkOrchid" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor8" Width="24" ToolTip="DarkOrchid"></Button>
                    <Button Background="MidnightBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor9" Width="24" ToolTip="MidnightBlue"></Button>
                    <Button Background="MediumBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor10" Width="24" ToolTip="MediumBlue"></Button>
                    <Button Background="Blue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor11" Width="24" ToolTip="Blue"></Button>
                    <Button Background="DodgerBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor12" Width="24" ToolTip="DodgerBlue"></Button>
                    <Button Background="Cyan" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor13" Width="24" ToolTip="Cyan"></Button>
                    <Button Background="Aqua" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor14" Width="24" ToolTip="Aqua"></Button>
                    <Button Background="SkyBlue" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor15" Width="24" ToolTip="SkyBlue"></Button>
                    <Button Background="ForestGreen" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor16" Width="24" ToolTip="ForestGreen"></Button>
                    <Button Background="YellowGreen" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor17" Width="24" ToolTip="YellowGreen"></Button>
                    <Button Background="GreenYellow" BorderBrush="AntiqueWhite" Height="14" Name="buttonColor18" Width="24" ToolTip="GreenYellow"></Button>
